.navbar_wrap {position: fixed; top: 0; padding: 0 26rpx; width: 100%; left: 0; z-index: 2; display: flex; justify-content: space-between; height: 90rpx; background-color:#fff;}
.navbar_wrap view {display:flex; width: 20%; justify-content: center; align-items: center;}
.navbar_wrap view.active {color: #E72839; border-bottom: 1rpx solid #E72839;}

.content_ul { padding-top: 90rpx;}
.content_ul .order_sn {display: flex; justify-content: space-between; padding: 12rpx 26rpx; font-size: 24rpx; color: #a3a3a3;background-color: #fff;}
.content_ul .order_sn text {}
.content_ul .content_li {padding: 12rpx 26rpx;}
.content_ul .content_li .img {float: left; width: 200rpx; height: 200rpx;}
.content_ul .content_li .img image {width: 100%; height: 100%;}
.content_ul .content_li .con {float: right; width: 474rpx;}
.content_li .con .tit {height: 90rpx;}
.content_li .con .isbaoy {margin-top: 26rpx; font-size: 26rpx; color: #a3a3a3; word-spacing: 1em;}
.content_li .con .msg {display: flex; align-items: baseline;}
.content_li .con .msg text:nth-child(1) {color: #E72839; font-size: 30rpx;}
.content_li .con .msg text:nth-child(2) {color: #a3a3a3; font-size: 24rpx; margin-left: 20rpx; text-decoration: line-through;}
.content_li .con .msg text:nth-child(3) {color: #a3a3a3; width: 200rpx; display: flex; font-size: 28rpx; flex: 1; justify-content: flex-end;}

.content_li_wrap {background-color: #f7f7f7; margin-bottom: 15rpx;}
.content_li_wrap .aside {background-color: #fff;}
.content_li_wrap .aside .top {display: flex; justify-content: flex-end; align-items: center; height: 78rpx; border-bottom: 1rpx solid #c4c4c4; padding-right: 26rpx;}
.content_li_wrap .aside .bottom {display: flex; justify-content: flex-end; align-items: center; height: 78rpx; padding-right: 26rpx;}
.content_li_wrap .aside .bottom text {padding: 15rpx 20rpx; font-size: 30rpx;}
.content_li_wrap .aside .bottom text.active {color: #E72839;}
